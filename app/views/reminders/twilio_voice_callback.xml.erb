<?xml version="1.0" encoding="UTF-8"?>
<Response>
    <Say>This call is to remind event <%= @reminder.description %></Say>
    <Gather timeout="10" finishOnKey="*" action="<%= twilio_keys_callback_reminder_url(@reminder) %>" method="GET">
        <Say>If you completed Press 1, If it is pending Press 3</Say>
    </Gather>
</Response>
